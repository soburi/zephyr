/dts-v1/;
#include <gigadevice-gd32vf103Xb.dtsi>

/ {
	model = "Sipeed Longan Nano";
	compatible = "sipeed,longan_nano";

	chosen {
		zephyr,console = &usart0;
		zephyr,shell-uart = &usart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};

&usart0 {
	current-speed = <115200>;
	status = "okay";
};

&usart1 {
	current-speed = <115200>;
	status = "okay";
};

&usart2 {
	current-speed = <115200>;
	status = "okay";
};

